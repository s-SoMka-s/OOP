    </Pane>
    <Pane fx:id="snakeInfo" prefHeight="600" prefWidth="200" style="-fx-background-color: blue;">
        <Label layoutX="69.0" layoutY="56.0" style="-fx-font-size: 16;" text="SCORE:" textFill="RED" />
        <Label fx:id="snakeInfoScore" textAlignment="CENTER" style="-fx-font-size: 16;" text="0" textFill="RED" />
        <Label textAlignment="CENTER" text="Snake length :" textFill="RED">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="level" layoutX="78.0" layoutY="177.0" text="3" textFill="RED">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
    </Pane>

</HBox>
